{% extends 'base.html.twig' %}

{% block title %}StaplingConfig index{% endblock %}

{% block body %}
    <h1>StaplingConfig index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Slug</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for stapling_config in stapling_configs %}
            <tr>
                <td>{{ stapling_config.id }}</td>
                <td>{{ stapling_config.slug }}</td>
                <td>
                    <a href="{{ path('app_stapling_config_show', {'id': stapling_config.id}) }}">show</a>
                    <a href="{{ path('app_stapling_config_edit', {'id': stapling_config.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_stapling_config_new') }}">Create new</a>
{% endblock %}
